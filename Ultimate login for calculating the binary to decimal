from back
int mod=1e9+7;
long long unsigned int decimalValue(Node *head)
{
     Node* temp=head;
     long long unsigned int c=0;
     while(head->next)
     {
        int i=head->data;
        c=((c+i)*2)%mod;
        head=head->next;
      }
      int m=head->data;
      c+=m;
      return c%mod;
}


